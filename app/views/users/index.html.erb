<h1>Users</h1>

    <% @users.each do |user| %>
        <div class="well">
        <p>Email: <%= user.email %></p>

        <p><%= link_to 'Show', user %> |
        <%= link_to 'Edit', edit_user_path(user) %> |
        <%= link_to 'Destroy', user, method: :delete, data: { confirm: 'Are you sure?' } %></p>
      </div>
    <% end %>

<br>

<%= link_to 'New User', new_user_path %>
